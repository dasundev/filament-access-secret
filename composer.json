{
    "name": "dasundev/filament-access-secret",
    "description": "This package provides a middleware for securing access to Filament by requiring a secret key to be provided in the URL.",
    "keywords": [
        "dasundev",
        "laravel",
        "filament",
        "secret",
        "login"
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dasundev\\FilamentAdminAccessSecret\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dasun Tharanga",
            "email": "hello@dasun.dev"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Dasundev\\FilamentAdminAccessSecret\\FilamentAdminAccessSecretServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
